<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE MudletPackage>
<MudletPackage version="1.001">
	<AliasPackage>
		<AliasGroup isActive="yes" isFolder="yes">
			<name>fedui</name>
			<script></script>
			<command></command>
			<packageName></packageName>
			<regex>^fedui</regex>
			<Alias isActive="yes" isFolder="no">
				<name>fedui toggle</name>
				<script>if matches[2] == "on" then
  fed2ui_toggle = "on"
  FEDUI.buildUI()
elseif matches[2] == "off" then
  fed2ui_toggle = "off"
  FEDUI.unbuildUI()
end</script>
				<command></command>
				<packageName></packageName>
				<regex>^fedui (on|off)$</regex>
			</Alias>
		</AliasGroup>
	</AliasPackage>
</MudletPackage>
